<div id="mt-plugin-video-uploader-modal" class="modal" role="dialog" aria-hidden="true" tabindex="-1">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title"><__trans phrase="Transcode Video"></h4>
      </div>
      <div class="modal-body">
        <div id="mt-plugin-video-uploader-console" class="card p-4"></div>
      </div>
      <div class="modal-footer">
        <input id="mt-plugin-video-uploader-filename" class="form-control" />
        <button
           type="submit"
           id="mt-plugin-video-uploader-upload"
           class="btn btn-primary action primary button">
          <__trans phrase="Upload">
        </button>
        <button
           type="button"
           id="mt-plugin-video-uploader-cancel"
           class="btn btn-default action button"
           data-dismiss="modal">
          <__trans phrase="Cancel">
        </button>
      </div>
    </div>
  </div>
</div>
